{
  "languageserver": {
    "qtpl": {
      "command": "/Users/adrian/github.com/a-h/qt-lsp/qt-lsp",
      "trace.server": "verbose",
      "filetypes": ["qtpl"]
    },
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "yaml.schemas": {
    "https://raw.githubusercontent.com/awslabs/goformation/master/schema/cloudformation.schema.json": "/*",
    "https://json.schemastore.org/github-action": ".github/*"
  },
  "yaml.customTags": [
    "!Ref",
    "!GetAtt",
    "!fn",
    "!And",
    "!If",
    "!Not",
    "!Equals",
    "!Or",
    "!FindInMap sequence",
    "!Base64",
    "!Cidr",
    "!Sub",
    "!GetAZs",
    "!ImportValue",
    "!Select",
    "!Split",
    "!Join sequence"
  ]
}
